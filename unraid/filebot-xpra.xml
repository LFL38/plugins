<?xml version="1.0"?>
<!--
$ curl -L https://raw.githubusercontent.com/filebot/plugins/master/unraid/filebot-xpra.xml -o /boot/config/plugins/dockerMan/templates/filebot-xpra.xml
-->
<Container version="2">
	<Name>filebot-xpra</Name>
	<Repository>rednoah/filebot:xpra</Repository>
	<Registry>https://hub.docker.com/r/rednoah/filebot/</Registry>
	<Network>bridge</Network>
	<MyIP/>
	<Shell>sh</Shell>
	<Privileged>false</Privileged>
	<Support>https://www.filebot.net/linux/unraid.html</Support>
	<Project>https://www.filebot.net/</Project>
	<Overview>FileBot Xpra WebUI</Overview>
	<Category>MediaApp:Video Status:Stable</Category>
	<WebUI>http://[IP]:[PORT:5454]/</WebUI>
	<TemplateURL/>
	<Icon>https://app.filebot.net/icon.png</Icon>
	<ExtraParams/>
	<PostArgs/>
	<CPUset/>
	<DonateText/>
	<DonateLink/>
	<Requires/>
	<Config Name="WebUI" Target="5454" Default="5454" Mode="tcp" Description="" Type="Port" Display="advanced" Required="true" Mask="false">5454</Config>
	<Config Name="Application Data" Target="/data" Default="data" Mode="rw" Description="Named volume for internal application data files." Type="Path" Display="advanced" Required="true" Mask="false">data</Config>
	<Config Name="User Files" Target="/volume1" Default="/mnt/user/Storage/" Mode="rw" Description="Bind mount that makes your files available via /volume1 from within the container." Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Storage/</Config>
</Container>
